{
  "key": "neeknnnjmpbdicafijkbnlmmlllfkhfh",
  "name": "SideNotes",
  "description": "Seamless Note Taking Interface",
  "version": "0.3",
  "manifest_version": 2,
  "icons": {"128": "icon.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "SideNotes",
    "default_popup": "html/popup.html"
  },
  "web_accessible_resources": [
    "chrome_oauth_receiver.html",
    "html/sidepanel.html"
  ],
  "background": {
    "page": "html/background.html"
  },
  "commands": {
    "toggle-sidebar": {
      "suggested_key": {
        "default": "Ctrl+N",
        "mac": "MacCtrl+N"
      },
    "description": "Toggle Sidebar"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>",
    "chrome-extension://afbonmgmjbiofanjpldocnjbdkpeodbj/html/options.html"
  ],
  "options_page": "html/options.html"
}
