{
  "key": "neeknnnjmpbdicafijkbnlmmlllfkhfh",
  "name": "SideNotes",
  "description": "Seamless Note Taking Interface",
  "version": "0.3",
  "manifest_version": 2,
  "icons": {"128": "icon.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "SideNotes",
    "default_popup": "html/popup.html"
  },
  "web_accessible_resources": [
    "chrome_oauth_receiver.html",
    "sidepanel.html"
  ],
  "background": {
    "page": "html/background.html"
  },
  "permissions": [
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "https://www.dropbox.com/",
    "https://api.dropbox.com/",
    "https://api-content.dropbox.com/",
    "storage"
  ]
}
